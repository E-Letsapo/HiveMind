CREATE TABLE university (
  university_id VARCHAR(50) PRIMARY KEY
);

CREATE TABLE student (
  student_id VARCHAR(50),
  university_id VARCHAR(50) REFERENCES university(university_id), 
  arrival_year VARCHAR(50),
  completion_year VARCHAR(50),
  PRIMARY KEY (university_id, student_id),
  year_of_study VARCHAR(50)
);

CREATE TABLE lecturer (
  lecturer_id VARCHAR(50),
  university_id VARCHAR(50) REFERENCES university(university_id),
  arrival_year VARCHAR(50),
  PRIMARY KEY (university_id, lecturer_id) 
); 

CREATE TABLE spersonal_info (
  spersonal_info_id VARCHAR(50) PRIMARY KEY,
  student_id VARCHAR(50),
  first_name VARCHAR(50),
  middle_name VARCHAR(50),
  last_name VARCHAR(50),
  id_number VARCHAR(20),
  district VARCHAR(100),
  contact VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id, student_id) REFERENCES student(university_id, student_id)
);

CREATE TABLE lpersonal_info (
  lpersonal_info_id VARCHAR(50) PRIMARY KEY,
  lecturer_id VARCHAR(50),
  first_name VARCHAR(50),
  middle_name VARCHAR(50),
  last_name VARCHAR(50),
  id_number VARCHAR(20),
  district VARCHAR(100),
  contact VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id, lecturer_id) REFERENCES lecturer(university_id, lecturer_id)
);

CREATE TABLE semester (
  semester_id VARCHAR(50) PRIMARY KEY,
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id)
);

CREATE TABLE course (
  course_id VARCHAR(50) PRIMARY KEY,
  course_name VARCHAR(50),
  credit_hours VARCHAR(50),
  semester_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (semester_id) REFERENCES semester(semester_id),
  FOREIGN KEY (university_id) REFERENCES university(university_id)
);

CREATE TABLE program (
  program_id VARCHAR(50) PRIMARY KEY,
  course_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (course_id) REFERENCES course(course_id)
);

CREATE TABLE department (
  department_id VARCHAR(50) PRIMARY KEY,
  program_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (program_id) REFERENCES program(program_id)
);

CREATE TABLE faculty (
  faculty_id VARCHAR(50) PRIMARY KEY,
  department_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (department_id) REFERENCES department(department_id)
);

CREATE TABLE sacademic_record (
  sacademic_record_id VARCHAR(40) PRIMARY KEY,
  student_id VARCHAR(50),
  faculty_id VARCHAR(50),
  department_id VARCHAR(50),
  program_id VARCHAR(50),
  course_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (program_id) REFERENCES program(program_id),
  FOREIGN KEY (department_id) REFERENCES department(department_id),
  FOREIGN KEY (faculty_id) REFERENCES faculty(faculty_id),
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (university_id, student_id) REFERENCES student(university_id, student_id)
);

CREATE TABLE lacademic_record (
  lacademic_record_id VARCHAR(40) PRIMARY KEY,
  lecturer_id VARCHAR(50),
  title VARCHAR(50),
  faculty_id VARCHAR(50),
  department_id VARCHAR(50),
  program_id VARCHAR(50),
  course_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (program_id) REFERENCES program(program_id),
  FOREIGN KEY (department_id) REFERENCES department(department_id),
  FOREIGN KEY (faculty_id) REFERENCES faculty(faculty_id),
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (university_id, lecturer_id) REFERENCES lecturer(university_id, lecturer_id)
);

CREATE TABLE result (
  result_id VARCHAR(50) PRIMARY KEY,
  academic_id VARCHAR(40),
  student_id VARCHAR(50),
  university_id VARCHAR(50),
  course_id VARCHAR(50) REFERENCES course(course_id),
  grade VARCHAR(10),
  lecture_comment VARCHAR(100),
  lecturer_id VARCHAR(50),
  FOREIGN KEY (university_id, student_id) REFERENCES student(university_id, student_id),
  FOREIGN KEY (university_id, lecturer_id) REFERENCES lecturer(university_id, lecturer_id) 
);

CREATE TABLE result_report (
  result_report_id VARCHAR(50) PRIMARY KEY,
  result_report_name VARCHAR(50),
  result_report_date DATE,
  pass_rate FLOAT,
  fail_rate FLOAT,
  explanation VARCHAR(200),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id)
);

CREATE TABLE budgets_report (
  budgets_report_id VARCHAR(50) PRIMARY KEY,
  budget_report_name VARCHAR(50),
  budget_report_date DATE,
  initial_amount FLOAT,
  used_amount FLOAT,
  balance FLOAT,
  explanation VARCHAR(200)
);

CREATE TABLE facilities_report (
  facilities_report_id VARCHAR(50) PRIMARY KEY,
  facilities_report_name VARCHAR(50),
  facilities_report_date DATE,
  explanation VARCHAR(200)
);

CREATE TABLE final_report (
  final_report_id VARCHAR(50) PRIMARY KEY,
  facilities_report_id VARCHAR(50),
  budgets_report_id VARCHAR(50),
  result_report_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (facilities_report_id) REFERENCES facilities_report(facilities_report_id),
  FOREIGN KEY (budgets_report_id) REFERENCES budgets_report(budgets_report_id), 
  FOREIGN KEY (result_report_id) REFERENCES result_report(result_report_id), 
  FOREIGN KEY (university_id) REFERENCES university(university_id)
);

CREATE TABLE library (
  library_id VARCHAR(50) PRIMARY KEY, 
  library_name VARCHAR(50), 
  university_id VARCHAR(50), 
  catalog VARCHAR(50), 
  usage_statistics VARCHAR(50), 
  library_events VARCHAR(50), 
  library_policies VARCHAR(50), 
  FOREIGN KEY (university_id) REFERENCES university(university_id)
);

CREATE TABLE library_information (
  library_information_id VARCHAR(50) PRIMARY KEY,
  student_id VARCHAR(50), 
  lecturer_id VARCHAR(50), 
  university_id VARCHAR(50),
  books_borrowed VARCHAR(50), 
  books_returned VARCHAR(50), 
  dates DATE, 
  library_id VARCHAR(50), 
  FOREIGN KEY (university_id, student_id) REFERENCES student(university_id, student_id),
  FOREIGN KEY (university_id, lecturer_id) REFERENCES lecturer(university_id, lecturer_id), 
  FOREIGN KEY (library_id) REFERENCES library(library_id) 
);

CREATE TABLE funding (
  funding_id VARCHAR(50) PRIMARY KEY, 
  student_id VARCHAR(50), 
  university_id VARCHAR(50),
  funding_name VARCHAR(50), 
  fund_purpose VARCHAR(150), 
  funding_duration DATE, 
  funding_status BOOLEAN, 
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (university_id, student_id) REFERENCES student(university_id, student_id), 
  funding_details VARCHAR(500)
);

CREATE TABLE project(
  project_id VARCHAR(50) PRIMARY KEY,
  project_name VARCHAR(65),
  project_description VARCHAR(300),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id)
);

CREATE TABLE fproject(
  fproject_id VARCHAR(50) PRIMARY KEY,
  project_id VARCHAR(50),
  student_id VARCHAR(50),
  lecturer_id VARCHAR(50),
  university_id VARCHAR(50),
  FOREIGN KEY (university_id) REFERENCES university(university_id),
  FOREIGN KEY (university_id, student_id) REFERENCES student(university_id, student_id),
  FOREIGN KEY (university_id, lecturer_id) REFERENCES lecturer(university_id, lecturer_id),
  FOREIGN KEY (project_id) REFERENCES project(project_id)
);

DROP TABLE IF EXISTS FProject CASCADE;
DROP TABLE IF EXISTS Project CASCADE;
DROP TABLE IF EXISTS Funding CASCADE;
DROP TABLE IF EXISTS Library_information CASCADE;
DROP TABLE IF EXISTS Library CASCADE;
DROP TABLE IF EXISTS MOET;
DROP TABLE IF EXISTS Final_report CASCADE;
DROP TABLE IF EXISTS Facilities_report CASCADE;
DROP TABLE IF EXISTS Budgets_report CASCADE;
DROP TABLE IF EXISTS Result_report CASCADE;
DROP TABLE IF EXISTS Result CASCADE;
DROP TABLE IF EXISTS Semester CASCADE;
DROP TABLE IF EXISTS Course CASCADE;
DROP TABLE IF EXISTS Program CASCADE;
DROP TABLE IF EXISTS Program CASCADE;
DROP TABLE IF EXISTS Courses CASCADE;
DROP TABLE IF EXISTS sacademic_record CASCADE;
DROP TABLE IF EXISTS lacademic_record CASCADE;
DROP TABLE IF EXISTS LPersonal_info CASCADE;
DROP TABLE IF EXISTS SPersonal_info CASCADE;
DROP TABLE IF EXISTS Lecturer CASCADE;
DROP TABLE IF EXISTS Student CASCADE;
DROP TABLE IF EXISTS Department CASCADE;
DROP TABLE IF EXISTS Faculty CASCADE;
DROP TABLE IF EXISTS University CASCADE;

